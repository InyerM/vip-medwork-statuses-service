{
  "name": "ms-statuses",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development APP_TYPE=RABBITMQ nest start --watch",
    "start:dev:http": "cross-env NODE_ENV=development APP_TYPE=HTTP nest start --watch",
    "start:dev:hybrid": "cross-env NODE_ENV=development APP_TYPE=HYBRID nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production APP_TYPE=RABBITMQ node dist/main",
    "start:prod:http": "cross-env NODE_ENV=production APP_TYPE=HTTP node dist/main",
    "start:prod:hybrid": "cross-env NODE_ENV=production APP_TYPE=HYBRID node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "node .husky/install.mjs || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "4.0.0",
    "@nestjs/bullmq": "11.0.2",
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.1",
    "@nestjs/microservices": "^11.0.13",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "11.0.1",
    "@nestjs/swagger": "11.0.6",
    "amqp-connection-manager": "4.1.14",
    "amqplib": "0.10.8",
    "axios": "1.10.0",
    "bullmq": "5.54.3",
    "chalk": "4.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "^16.4.7",
    "express": "4.21.2",
    "mongoose": "^8.13.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@commitlint/types": "19.8.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.1",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.7",
    "@types/supertest": "6.0.2",
    "cross-env": "7.0.3",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.2",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "57.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.5.0",
    "prettier": "3.4.2",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "type-fest": "4.39.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core"
    ]
  }
}
